{
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=3600, must-revalidate"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}